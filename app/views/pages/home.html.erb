        <!-- PG Time Machine Stats -->
           <div class="section parallax nomargin notopborder" style="background: url('aseets/parallax/3.jpg') center center; padding: 100px 0;" data-stellar-background-ratio="0.3">
                    <div class="container-fluid center clearfix">

                        <div class="heading-block">
                            <h2>Intro stat about bar crwal</h2>
                            <span data-animate="bounceIn" data-delay="5000">Show off Different Elements with a Different Touch.</span>
                            <div class="clear"></div>
                        </div>
                        
                        <div class="col_one_fourth nobottommargin" data-animate="bounceIn" data-delay="100">
                            <div class="counter counter-large counter-lined"><span data-from="0" data-to="300" data-refresh-interval="1.2" data-speed="200"></span></div>
                            <h5>People Expected</h5>
                        </div>

                        <div class="col_one_fourth nobottommargin" data-animate="bounceIn" data-delay="1000">
                            <div class="counter counter-large counter-lined"><span data-from="0" data-to="4" data-refresh-interval="10" data-speed="100"></span></div>
                            <h5>Bars</h5>
                        </div>


                        <div class="col_one_fourth nobottommargin" data-animate="bounceIn" data-delay="1500">
                            <div class="counter counter-large counter-lined"><span data-from="0" data-to="3" data-refresh-interval="10" data-speed="100"></span></div>
                            <h5>Buses</h5>
                        </div>

                        <div class="col_one_fourth nobottommargin col_last" data-animate="bounceIn" data-delay="2000">
                            <div class="counter counter-large counter-lined"><span data-from="0" data-to="1" data-refresh-interval="10" data-speed="100"></span></div>
                            <h5>Day</h5>
                        </div>

                    </div>
                </div>
        <!-- PG Time Machine Stats -->

         <!-- User Comments Start -->

            <div class="section">
                <div class="container clearfix">
                    <h3 class="center">Comments</h3>

                    <div id="oc-testi" class="owl-carousel testimonials-carousel">

                        <% @comments.each do |comment| %>
                            <% #if !comment.comment.blank? %>
                                <div class="oc-item">
                                    <div class="testimonial">
                                        <div class="testi-image">
                                            <%= image_tag(comment.user.image)%>
                                        </div>
                                        <div class="testi-content">
                                            <p><%= comment.comment %></p>
                                            <div class="testi-meta">
                                                <%= comment.user.name %>
                                                <span><%= comment.created_at.strftime("%D") %></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                             <% #end %>
                        <% end %>

                    </div>

                    <script type="text/javascript">

                        jQuery(document).ready(function($) {

                            var ocTesti = $("#oc-testi");

                            ocTesti.owlCarousel({
                                margin: 10,
                                autoplay: true,
                                loop: true,
                                nav: true,
                                autoplayHoverPause: false,
                                navText: ['<i class="icon-angle-left"></i>','<i class="icon-angle-right"></i>'],
                                dots: true,
                                responsive:{
                                    0:{ items:1 },
                                    768:{ items:2 },
                                    992:{ items:4 }
                                }
                            });

                        });

                    </script>

                </div>
            </div>

            <!--- user Comments End -->


